{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@interfaces/*": [
        "./src/interfaces/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}